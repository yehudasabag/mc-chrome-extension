{
  "name": "MC-Chrome-Extension",
  "description": "Tool that enable to deal with MC from the chrome.",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": ["webview", "http://*/"],
  "app": {
    "background": {
      "scripts": [
        "Common/minevents.js",
        "MCCommunication/CommonSettings.js",
        "MCCommunication/AppMgr.js",
        "MCCommunication/DeviceMgr.js",
        "MCCommunication/ConnectionMgr.js",
        "MCCommunication/CommandsMgr.js",
        "MCCommunication/JobMgr.js",
        "background.js"
      ]
    }
  },
  "icons": { "16": "MClogo16.png", "128": "MClogo128.png" }
}