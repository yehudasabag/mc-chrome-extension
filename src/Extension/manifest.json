{
  "name": "MC-Chrome-Extension",
  "description": "Tool that enable to deal with MC from the chrome.",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": ["webview"],
  "webview": {
    "partitions": [
      {
        "name": "localPackage",
        "accessible_resources": [ "*.js", "*.html" ]
      }
    ]
  },
  "app": {
    "background": {
      "scripts": [
        "Common/minevents.js",
        "background.js"
      ]
    }
  },
  "icons": { "16": "MClogo16.png", "128": "MClogo128.png" }
}